#include "stm32_f446xx.h"
#include "i2c.h"
#include "ssd1306.h"
#include "gfx.h"

int main(void)
{
    I2C1_Init();
    SSD1306_Init();

    SSD1306_Clear();

    SSD1306_DrawString(10, 5, "HELLO STM32");
    GFX_DrawRect(0, 0, 127, 31);
    GFX_DrawLine(0, 16, 127, 16);

    SSD1306_Update();

    while (1);
}
